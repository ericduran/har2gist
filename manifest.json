{
  "name": "har2gist",
  "description": "Saves HAR to Gist and gives you a link to view the waterfall.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "upload.png"
  },
  "devtools_page": "devtools.html"
}
